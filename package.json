  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "postinstall": "node postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codejovz/react-svg-inliner.git"
  },
  "author": "Jorge Varela Zamora <jorgevarelazamora@gmail.com>",
  "bugs": {
    "url": "https://github.com/codejovz/react-svg-inliner/issues"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "react",
    "svg",
    "nextjs",
    "inline-svg"
