{
  "name": "react-svg-inliner",
  "version": "1.2.0",
  "description": "Inline SVG loader for React and Next.js client components",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
